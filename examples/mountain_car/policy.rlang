import vocab.json

Action go_right := 2
Action go_left := 0
Action no_op := 1

Factor position := S[0]
Factor velocity := S[1]

Policy main:
    if position < 0 and velocity == 0:
        Execute go_right
    elif position > 0 and velocity == 0:
        Execute go_left
    elif position == 0 and velocity == 0:
        Execute go_left
    else:
        Execute no_op