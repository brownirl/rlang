import vocab.json

Action go_right := 1
Action go_left := -1
Action no_op := 0

Factor position := S[0]
Factor velocity := S[1]

Policy p1:
    if position < 0 and velocity == 0:
        Execute go_right
    elif position > 0 and velocity == 0:
        Execute go_left
    elif position == 0 and velocity == 0:
        Execute go_left
    else:
        Execute no_op