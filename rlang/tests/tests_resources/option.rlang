Option go_to_workshop_2:
    init(True)
        Find
    until at_workshop2

Option build_bridge:
    init(wood >= 1 and iron >=1 and at_workshop2)
        Execute use
    until delta_bridge > 0

Option sit_down:
    init 1 == 1
        if has_iron:
            if has_bench:
                Execute sit
    until at_workshop2